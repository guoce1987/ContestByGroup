    <definition name="system/admin/index" extends="default">
        <put-attribute name="title" value="首页"/>
        <put-attribute name="body" value="/WEB-INF/jsp/tilesTest/index.jsp"/>
         <put-list-attribute name="javascripts" inherit="true">
            <add-attribute value="static/js/myjs/tileTest/index.js?v=1.0.0"/>
        </put-list-attribute>
    </definition>
     <definition name="contestResult/list" extends="default">
