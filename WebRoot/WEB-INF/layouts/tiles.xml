    <definition name="CustomerDetail" extends="default">
        <put-attribute name="title" value="DDDDDD"/>
        <put-attribute name="body" value="/WEB-INF/jsp/tilesTest/CustomerDetail.jsp"/>
    </definition>
    <definition name="tilesTest/templet" extends="default">
        <put-attribute name="title" value="FFFFFF"/>
