	<select id="listAllPowerIndexForChart" resultMap="powerIndexMapForChart"  parameterType="pd">
			select 
			groupName,
			RJ_GeneratePowerAvg  
			from vw_ContestResult
			where year(StatDate)= #{year} and month(StatDate) = #{month}
