	</select>
	<!-- 列出所有的安全指标(表) -->
	<select id="listAllSecurityIndexForGrid" resultMap="securityIndexMapForGrid"  parameterType="pd">
			SELECT 	
			ID,
		    CheckDate, 
		    GroupName, 
		 	ItemName, 
		    Score, 
		    Money, 
		    Memo,
		    ContestItem
			FROM Vw_RewardPunishLog
			WHERE ContestItemid=1 AND YEAR(CheckDate)= #{year} AND MONTH(CheckDate) = #{month}
