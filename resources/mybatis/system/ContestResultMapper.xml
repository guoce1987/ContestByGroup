	<select id="listAllAuxPowerRatioForGrid" resultMap="auxPowerRatioMapForGrid"  parameterType="pd">
			SELECT 
				cc.id,
				cc.DutyID, 
				CONVERT(varchar(100), cc.StatDate, 23) as StatDate,
				cc.DutyName, 
				cc.GroupName, 
				cc.RJ_GeneratePower,
				cc.RJ_TotalPlantUsePowerRatio*100 AS RJ_TotalPlantUsePowerRatio,  
				cc.RJ_SuplyPower,
				cc.XL2213PowerFLow,
				ISNULL(lg.BreakPower, 0) BreakPower
			FROM vw_CoalCost cc
			left join 
			(
				select 
					BreakDate,
					DutyClass,
					sum(BreakPower) BreakPower
				from BreakPowerLog 
				group by BreakDate,DutyClass
			) lg on lg.BreakDate=cc.statDate and lg.DutyClass=cc.DutyID
