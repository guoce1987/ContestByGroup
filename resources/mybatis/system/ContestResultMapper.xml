	<!-- 列出所有的电量指标(表) -->
	<select id="listAllPowerIndexForGrid" resultMap="powerIndexMapForGrid"  parameterType="pd">
			SELECT 
				cc.DutyID, 
				CONVERT(varchar(100), cc.StatDate, 23) as StatDate,
				cc.DutyName, 
				cc.GroupName, 
				cc.RJ_GeneratePower,
				ISNULL(sum(bpl.BreakPower), 0) BreakPower
			FROM vw_CoalCost cc
			left join BreakPowerLog bpl on cc.statDate=bpl.BreakDate and cc.DutyID=bpl.DutyClass
			WHERE (YEAR(cc.statDate) = #{year}) AND (MONTH(cc.statDate) = #{month}) 
			group by cc.statDate,cc.DutyID,cc.DutyName,cc.GroupName,cc.RJ_GeneratePower
			Order by cc.Statdate desc,cc.Dutyid desc
	</select>
	
