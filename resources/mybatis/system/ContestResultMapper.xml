	<resultMap type="PowerIndexForChart" id="powerIndexMapForChart">
		<result column="GroupName" property="GroupName"/>
		<result column="RJ_GeneratePowerAvg" property="RJ_GeneratePowerAvg"/>
	</resultMap>
	
