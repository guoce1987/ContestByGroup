		<result column="DutyID" property="DutyID"/>
		<result column="StatDate" property="StatDate"/>
		<result column="DutyName" property="DutyName"/>
		<result column="GroupName" property="GroupName"/>
		<result column="RJ_GeneratePower" property="RJ_GeneratePower"/>
		<result column="BreakPower" property="BreakPower"/>
	</resultMap>
	<resultMap type="BreakPowerForGrid" id="BreakpowerMapForGrid">
		<result column="ID" property="id"/>
		<result column="BreakDate" property="breakDate"/>
		<result column="DutyName" property="dutyName"/>
		<result column="DutyClass" property="dutyClass"/>
		<result column="groupName" property="groupName"/>
		<result column="BreakPower" property="breakPower"/>
		<result column="CreateDate" property="createDate"/>
	</resultMap>
	<resultMap type="GroupInfo" id="GroupInfoMap">
		<result column="groupName" property="groupName"/>
	</resultMap>
	<select id="queryGroupInfo" resultMap="GroupInfoMap"  parameterType="pd">
		select 
			d.groupName 
		from VwShiftRule r 
		left join GroupDic d on r.GroupId=d.id
		where Work_Dt=#{statDate} and DutyId=#{dutyId}
	</select>
	<!-- 列出所有的违规电量（表） -->
	<select id="listAllBreakPowerForGrid" resultMap="BreakpowerMapForGrid"  parameterType="pd">
		SELECT b.ID
	      ,CONVERT(varchar(100), BreakDate, 23) as BreakDate
	      ,b.DutyClass
	      ,d.DutyName
	      ,b.BreakPower
	      ,b.CreateDate
		  ,gd.groupName
	  	FROM BreakPowerLog b
	  	left join DutyDic d on b.DutyClass=d.dutyId
		left join VwShiftRule vr on b.BreakDate=vr.Work_Dt and b.DutyClass=vr.DutyId
	  	left join GroupDic gd on vr.GroupId=gd.id
	  	where year(BreakDate)= #{year} and month(BreakDate) = #{month} and day(BreakDate)=#{day} and DutyClass=#{dutyId}
	  	order by CreateDate desc
	</select>
	<resultMap type="BreakPowerForSum" id="breakpowerSumInfoMap">
		<result column="sumDuty" property="sumDuty"/>
		<result column="sumDay" property="sumDay"/>
		<result column="sumMonth" property="sumMonth"/>
	</resultMap>
	<select id="breakpowerSumInfo" resultMap="breakpowerSumInfoMap"  parameterType="pd">
		select 
			ISNULL((select sum(BreakPower) from BreakPowerLog where BreakDate=#{breakDate} and DutyClass=#{dutyId}),0) as sumDuty,
			ISNULL((select sum(BreakPower) from BreakPowerLog where BreakDate=#{breakDate}),0) as sumDay,
			ISNULL((select sum(BreakPower) from BreakPowerLog where year(BreakDate)=year(CONVERT(date, #{breakDate})) and month(BreakDate)=month(CONVERT(date, #{breakDate}))),0)  as sumMonth
	</select>
	<!-- 删除违规电量deleteBreakpower -->
	<select id="deleteBreakpower" parameterType="pd" resultType="pd" useCache="false"> 
		  delete from BreakPowerLog where id = #{id}
	</select>
	<!-- 保存违规电量 -->
	<select id="saveBreakPowerLog" parameterType="pd" resultType="pd" useCache="false"> 
		  insert into BreakPowerLog 
		 	(	
	  	  		BreakDate
		      ,DutyClass
		      ,BreakPower
		      ,CreateDate
      		) values 
      		(
      			#{breakDate},
      			#{dutyId},
      			#{breakpower},
      			GETDATE()
      		)
	</select>
