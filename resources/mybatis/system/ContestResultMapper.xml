	<select id="listAllKKS" parameterType="pd" resultType="pd" useCache="false">
		SELECT distinct '【'+KKS+'】 '+ Description as value, KKS as kks FROM BreakPointDic	
	</select>
	
	<!-- 新增一条考核管理项 -->
	<select id="saveOneContestItem" parameterType="pd" resultType="pd" useCache="false"> 
		  insert into RewardPunishDic 
		  	  (ContestItemID
			  ,IsTag
			  ,IsDelete
			  ,itemName
			  ,StartStopType
			  ,cent
			  ,money
			  ,ratio
			  ,memo
			  ,StartStop
			  ,listorder) values (#{ContestItemID},#{IsTag},#{IsDelete},#{itemName},#{StartStopType},#{cent},#{money},#{ratio},#{memo},#{StartStop},#{listorder})
	</select>
	
	<!-- 新增一条考核管理记录 -->
	<select id="saveItemInput" parameterType="pd" resultType="pd" useCache="false"> 
		  insert into RewardPunishLog 
		  	  (GroupID
		      ,Unit
		      ,CheckDate
		      ,ItemID
		      ,Score
		      ,Money
		      ,Memo
		      ,LogDate
		      ,CreateUser) values (#{GroupID},#{Unit},#{CheckDate},#{ItemID},#{Score},#{money},#{memo},CURRENT_TIMESTAMP,#{CreateUser})
	</select>
	
	<!-- 取最新的考核管理记录 -->
	<select id="listThisItemInput" parameterType="pd" resultType="pd" useCache="false"> 
		  SELECT TOP 1 id
		      ,GroupID
		      ,Unit
		      ,CONVERT(VARCHAR(20),CheckDate,20) as CheckDate
		      ,ItemID
		      ,Score
		      ,Money
		      ,Memo
		      ,CONVERT(VARCHAR(20),LogDate,20) as LogDate
		      ,CreateUser
		  FROM RewardPunishLog order by id desc
	</select>
	
	<!-- 根据ID查询一条考核管理记录 -->
	<select id="listThisItemInputByID" parameterType="pd" resultType="pd" useCache="false"> 
		  SELECT id
		      ,GroupID
		      ,Unit
		      ,CONVERT(VARCHAR(20),CheckDate,20) as CheckDate
		      ,ItemID
		      ,Score
		      ,Money
		      ,Memo
		      ,CONVERT(VARCHAR(20),LogDate,20) as LogDate
		      ,CreateUser
		  FROM RewardPunishLog where ID=#{ID}
	</select>
	
	<!-- 根据ID更新一条考核管理记录 -->
	<select id="updateItemInput" parameterType="pd" resultType="pd" useCache="false"> 
		  UPDATE RewardPunishLog SET GroupID=#{GroupID},Unit=#{Unit},CheckDate=#{CheckDate},ItemID=#{ItemID},Score=#{Score},Money=#{money},Memo=#{memo},LogDate=CURRENT_TIMESTAMP where ID=#{ID}
	</select>
	
	<!-- 根据ID删除一条考核管理记录 -->
	<select id="deleteItemInput" parameterType="pd" resultType="pd" useCache="false"> 
		  DELETE
		  FROM RewardPunishLog where ID=#{ID}
	</select>
	
	<select id="runPr" parameterType="pd" resultType="pd" statementType="CALLABLE" useCache="false">
		{ call sp_DutyTask(#{statDate}) }
	</select>
</mapper>